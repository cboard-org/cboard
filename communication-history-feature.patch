+    // All entries should have the same user and session
+    trackedEntries.forEach(entry => {
+      expect(entry.userId).toBe(userId);
+      expect(entry.sessionId).toBe(sessionId);
+      expect(entry.timestamp).toBeDefined();
+    });
+  });
+});
