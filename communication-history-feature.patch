+      const tile = {
+        id: 'tile1',
+        label: 'Water',
+        image: 'water.png',
+        boardId: 'board1'
+      };
+
+      trackSymbolSelection(tile, 'user@example.com', 'session123')(dispatch);
+
+      expect(dispatch).toHaveBeenCalledWith(
