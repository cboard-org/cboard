+    it('should handle ADD_COMMUNICATION_ENTRY', () => {
+      const entry = {
+        id: '123',
+        type: COMMUNICATION_ENTRY_TYPES.SYMBOL,
+        label: 'Hello',
+        timestamp: '2024-01-15T10:30:00.000Z'
+      };
